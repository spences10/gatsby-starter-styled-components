{
  "name": "gatsby-starter-styled-components",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Scott Spence <yo@scottspence.dev>",
  "dependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "gatsby": "3.2.0",
    "gatsby-image": "3.2.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "3.2.0",
    "gatsby-plugin-offline": "4.2.0",
    "gatsby-plugin-react-helmet": "4.2.0",
    "gatsby-plugin-sharp": "3.2.0",
    "gatsby-plugin-styled-components": "4.2.0",
    "gatsby-source-filesystem": "3.2.0",
    "gatsby-transformer-sharp": "3.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "styled-components": "5.2.1",
    "styled-reset": "4.3.4"
  },
  "devDependencies": {
    "husky": "6.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "pretty-quick",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "main": "gatsby-browser.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/gatsby-starter-styled-components.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/gatsby-starter-styled-components/issues"
  },
  "homepage": "https://github.com/spences10/gatsby-starter-styled-components#readme"
}
